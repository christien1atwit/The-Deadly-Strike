	Vcurrent_lane==0:
				HitboxRequires(BackLane)
				Hitbox(1500, 21000, 20000, 30000)
			endif
			Vcurrent_lane==1:
				HitboxRequires(MidLane)
				Hitbox(1500, 21000, 20000, 30000)
			endif
			Vcurrent_lane==2:
				HitboxRequires(FrontLane)
				Hitbox(1500, 21000, 20000, 30000)
			endif
endif

F10:
	LAttackHasHit:
		AddMomentum(200)
	endif
endif
